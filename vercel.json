{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm ci",
  "regions": ["syd1"],
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1"
    }
  },
  "functions": {
    "src/app/**/*.tsx": {
      "maxDuration": 10
    },
    "src/app/api/**/*.ts": {
      "maxDuration": 10
    }
  }
}